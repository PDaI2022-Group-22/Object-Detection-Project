BoxLayoutExample:
#This file defines the layout

<BoxLayoutExample>:
    orientation: "vertical"
    Label:
        text: "Object Detection App"
        size_hint: 1, .20
        pos: "200dp", "200dp"
        color: 0, 0, 1, 1 #values=red,green,blue,visibility
    GridLayoutExample:
        cols: 2
        padding: 20, 20, 20, 20 #left, top, right, bottom
        Button:
            text: "RockPaperScissors"
            on_press: root.AA()
            background_normal: 'RockPaperScissorsLogo.png'
        Button:
            text: "SnakeGame"
            color: 0, 0, 0, 1 #values=red,green,blue,visibility
            on_press: root.BB()
            background_normal: 'Snake.png'
        Button:
            text: "C"
        Button:
            text: "Taste Of Game"
            on_press: root.DD()
            background_normal:'Jaakko.png'
        Button:
            text: "Emotion Checker"
            on_press: root.EE()
        Button:
            text: "F"    

        Button:
            text: "Test"
            

            
                
                
               

             

